{
    "name": "Tunora - Music player",
    "short_name": "Tunora",
    "description": "A progressive web app that allows users to experience the best features of web and mobile apps.",
    "version": "0.1.0",
    "manifest_version": 1,
    "start_url": "/tunora",
    "scope": "/tunora",
    "display": "standalone",
    "background_color": "#4444ff",
    "theme_color": "#4433ff",
    "orientation": "portrait",
    "icons": [
      {
        "src": "assets/icons/192.png",
        "sizes": "192x192",
        "type": "image/png"
      },
      {
        "src": "assets/icons/512.png",
        "sizes": "512x512",
        "type": "image/png"
      },
      {
        "src": "assets/icons/128.png",
        "sizes": "128x128",
        "type": "image/png"
      },
      {
        "src": "assets/icons/1024.png",
        "sizes": "1024x1024"
      }
    ]
}  